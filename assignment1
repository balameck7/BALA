package week1.day2;

public class assignment {
		
	 public static void main(String[] args) {
		
	 }
	 
	 public int sum(int a, int b) {
		 return a+b;
	 }
	 public int sub(int a, int b) {
		 return a-b;
	 }
	 public int mul(int a, int b) {
		 return a*b;
	 }
	 public int div(int a, int b) {
		 return a%b;
	 }
}
