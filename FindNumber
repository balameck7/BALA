package week3.day6;

import java.util.ArrayList;
import java.util.List;

public class FindNumber {

	public static void main(String[] args) {
		List<Integer> intlist=new ArrayList<Integer>();
		intlist.add(1);
		intlist.add(23);
		intlist.add(45);
		intlist.add(66);
		intlist.add(88);
		intlist.add(45);
		int count=0;
        for(int eachNum:intlist){
            if(eachNum==45){
                count++;
            }
            }
        System.out.println("Number of times 45 is" +count );
	}
}
