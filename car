package week1.day2;

public class car {
	
	public void printCarBrandName() {
		System.out.println("BMW");
	}
    public String getCarColor() {
    	return "Blue";
    }
    public int getCarNumber() {
    	return 729192;
    }
    public void addTwoNumbers (int a, int b) {
    	int c = a+b;
    }
    public int multiplyTwoNumber (int a, int b) {
    	int c = a*b;
    	return c;
    
    }
    public int divideTwoNumber(int c, int d) {
    	int f = c/d;
    	return f;
    }
    public static void main (String[] args) {
    	car obj = new car();
    	obj.printCarBrandName();
    	System.out.println(obj.getCarColor());
    	System.out.println(obj.getCarNumber());
    	obj.addTwoNumbers( 5, 7);
    	int mul = obj.multiplyTwoNumber ( 7, 5);
    	System.out.println("Multiplied value is" + mul);
    	int div = obj.divideTwoNumber(50 , 10);
    	System.out.println("Quotient is " +div);
    	
    }
}
